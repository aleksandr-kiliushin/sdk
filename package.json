{
  "devDependencies": {
    "@eslint/js": "9.28.0",
    "@stylistic/eslint-plugin": "4.4.1",
    "@types/node": "22.15.29",
    "@types/react": "19.1.8",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-perfectionist": "4.14.0",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-dom": "1.52.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-react-x": "1.52.2",
    "globals": "16.2.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.0",
    "prettier": "3.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.0",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/sdk.es.js",
      "require": "./dist/sdk.cjs.js"
    },
    "./base-configs/eslint-react": {
      "types": "./base-configs/types/eslint.react.base.config.d.cts",
      "require": "./base-configs/eslint.react.base.config.cjs"
    },
    "./base-configs/prettier": {
      "types": "./base-configs/types/prettier.base.config.d.cts",
      "require": "./base-configs/prettier.base.config.cjs"
    },
    "./base-configs/tsconfig-react": "./base-configs/tsconfig.react.base.json",
    "./styles.css": "./dist/sdk.css"
  },
  "files": [
    "./base-configs",
    "./dist"
  ],
  "name": "aleksandr-kiliushin-sdk",
  "peerDependencies": {
    "classnames": "^2.5.1",
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "react-router-dom": ">=5.0.0 <8.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist ./base-configs/types && tsc --project ./base-configs/_tsconfig.build.json && vite build",
    "build:watch": "rm -rf ./dist && vite build --watch",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "format:js": "eslint --fix",
    "format:other": "prettier \"./**/*.{css,json,md}\" --write",
    "lint:js": "eslint",
    "publish:alpha": "npm run build && npm publish --tag alpha",
    "publish:latest": "npm run build && npm publish --tag latest"
  },
  "type": "module",
  "version": "1.0.0-alpha.0"
}
